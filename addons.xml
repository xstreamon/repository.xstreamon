     <icon>icon.png</icon>
     <fanart>fanart.jpg</fanart>
     <screenshot>resources/art/screenshot-01.jpg</screenshot>
     <screenshot>resources/art/screenshot-02.jpg</screenshot>
   </assets>
 </extension>
